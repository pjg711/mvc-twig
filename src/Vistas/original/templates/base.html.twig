<!DOCTYPE html>
<html ng-app="CifasisApp">
<!--[if lt IE 7]>
    <html class="no-js lt-ie9 lt-ie8 lt-ie7">
<![endif]-->
<!--[if IE 7]>
    <html class="no-js lt-ie9 lt-ie8">
<![endif]-->
<!--[if IE 8]>
    <html class="no-js lt-ie9">
<![endif]-->
<!--[if gt IE 8]>
    <html class="no-js">
<![endif]-->
{% block head %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Hola Mundo!{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Homepage Institucional CIFASIS" />
    <meta name="keywords" content="INFORMATICA CIFASIS CONICET">
    <meta name="author" content="FREEHTML5.CO" />
    {# Facebook and Twitter integration #}
    <meta property="og:title" content=""/>
    <meta property="og:image" content=""/>
    <meta property="og:url" content=""/>
    <meta property="og:site_name" content=""/>
    <meta property="og:description" content=""/>
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />
    {# favicon.ico #}
    <link rel="shortcut icon" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_img }}/favicon.ico">
    {% block estilos %}
    {# GOOGLE Fonts #}
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,600,400italic,700' rel='stylesheet' type='text/css'>
    {# Animate.css #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/animate.css">
    {# Icomoon Icon Fonts #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/icomoon.css">
    {# Simple Line Icons #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/simple-line-icons.css">
    {# Fontawesome #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/fortawesome/font-awesome/css/font-awesome.min.css">
    {# Preloader #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/cifasis/preloader.css">
    {# Accordion #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/accordion.css">
    {# SidebarTransitions #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/normalize.css">
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/icons.css">
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/component.css">
    {# Angular-toaster #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/bower_components/AngularJS-Toaster/toaster.min.css"></script>
    {# Responsive News Ticker #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/responsive-news-ticker/css/site.css">
    {# Bootstrap #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/components/bootstrap/css/bootstrap.min.css">
    {# Estilos propios #}
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/cifasis/general.css">
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/cifasis/navbar.css">
    <link rel="stylesheet" href="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_css }}/cifasis/colores.css">
    {% endblock %}
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/respond.min.js"></script>
    <![endif]-->
{% endblock %}

{% block body %}
    {% block preloader %}
        {# start preloader #}
        <div id="loader-wrapper">
            <div class="logo"></div>
            <div id="loader"></div>
        </div>
        {# end preloader #}
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    {% endblock %}

    {% block navbar %}

    {% endblock %}

    {% block footer %}
        <footer>
            <div class="row row-bottom-padded-sm">
                <div class="col-md-12 text-center">

                </div>
            </div>
        </footer>
    {% endblock %}

    {% block js %}
        {# AngularJS y app.js #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/components/angular/angular.js"></script>
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/components/angular/angular-animate.js"></script>
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/components/angular/angular-sanitize.js"></script>
        {# <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/vendor/components/angular-ui-router/release/angular-ui-router.js"></script> #}
        {# Satellizer #}
        {# <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/bower_components/satellizer/dist/satellizer.min.js"></script> #}
        {# app.js #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/cifasis/app.js"></script>
        {# jQuery #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/jquery.min.js"></script>
        {# jQuery Easing #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/jquery.easing.1.3.js"></script>
        {# Bootstrap #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/bootstrap.min.js"></script>
        {# Waypoints #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/jquery.waypoints.min.js"></script>
        {# Main JS #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/cifasis/main.js"></script>
        {# Modernizr JS #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/modernizr-2.6.2.min.js"></script>
        {# SidebarTransitions #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/classie.js"></script>
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/sidebarEffects.js"></script>
        {# Angular-toaster #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/bower_components/AngularJS-Toaster/toaster.min.js"></script>
        {# SearchableNavbar #}
        {# <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/SearchableNavbar/assets/js/bootstrap-modal.js"></script> #}
        {# Responsive News Ticker  #}
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/responsive-news-ticker/scripts/jquery.bootstrap.newsbox.min.js" type="text/javascript"></script>
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/responsive-news-ticker/scripts/news1.js" type="text/javascript"></script>
        <script src="{{ config.PROTOCOLO }}{{ config.WWW_ROOT }}/{{ config.FOLDER_VIEW }}/{{ config.FOLDER_STYLE }}/{{ pagina.folder_js }}/responsive-news-ticker/scripts/vinculos1.js" type="text/javascript"></script>
    {% endblock %} {# fin js #}
{% endblock %} {# fin body #}
</html>
